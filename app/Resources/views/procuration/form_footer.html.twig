{% for warning in form.vars.warnings %}
    <div class="form--warning">
        {{ warning.messageTemplate|trans(warning.messageParameters, 'validators')|raw }}
    </div>
{% endfor %}

<div class="text--center b__nudge--top-large">
    <button type="submit" class="btn btn--blue">
        {{- 'must_confirm' == form.state.vars.value ? 'Je confirme ma nouvelle demande' : 'Je continue' -}}
    </button>
</div>

{{ form_rest(form) }}
