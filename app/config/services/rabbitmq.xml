<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <!-- Mailjet -->
        <service id="app.consumer.mailjet.referent" class="AppBundle\Consumer\Mailjet\ReferentMessageMailjetConsumer">
            <argument type="service" id="service_container" />
        </service>

        <!-- Referents managed users dumper -->
        <service id="app.consumer.referent_managed_users_dumper" class="AppBundle\Consumer\ReferentManagedUsersDumperConsumer">
            <argument type="service" id="service_container" />
        </service>

    </services>
</container>
